}                       
else
{

      <!--begin::Card body-->
                    <div class="card-body py-3">
                        <div class="table-responsive">
                            <!--begin::Table-->
                            <table class="table align-middle gs-0 gy-4" >
                                <!--begin::Table head-->
                                <thead>
                                    <tr class="fw-bold text-muted bg-light">
                                        <th class="ps-4 min-w-125px rounded-start">Start Date</th>
                                        <th class="min-w-125px"># of Copies</th>
                                        <th class="min-w-150px">Total Delay (Days)</th>
                                        <th class="min-w-150px">Added-on</th>
                                        <th class="min-w-125px text-end rounded-end pe-4">Add On</th>
                                    </tr>
                                </thead>
                                <!--end::Table head-->
                                <!--begin::Table body-->
                                <tbody>
                                    @foreach (var rentel in Model)
                                    {
                                        <tr>
                                            <td>
                                              <div class="d-flex align-items-center ps-3">
                                                    @rentel.StartDate.ToString("d MMM, yyyy")      
                                               </div>  
                                            </td>
                                            
                                            <td>
                                                @rentel.NumberOfCopies
                                            </td>

                                             <td>
                                                 @rentel.TotalDelayInDays

                                            </td>
                                            
                                            <td>
                                                @rentel.CreatedOn

                                            </td>
                                            
                                            <td class="text-end pe-4">

                                                <a class="btn btn-primary btn-sm" asp-controller="Rentels" asp-action="Details" asp-route-id="@rentel.Id">Details</a>
                                            </td>

                                        </tr>

                                    }
                                </tbody>
                                <!--end::Table body-->
                            </table>
                            <!--end::Table-->
                        </div>
                    </div>
                    <!--end::Card body-->



}